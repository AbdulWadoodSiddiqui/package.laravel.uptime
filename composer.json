{
    "name": "uptime/monitoring",
    "description": "Laravel package for uptime monitoring and API logging",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Uptime Monitor",
            "email": "support@uptimemonitor.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/validation": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Uptime\\Monitoring\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uptime\\Monitoring\\MonitoringServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
